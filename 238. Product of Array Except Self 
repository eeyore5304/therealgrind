class Solution(object):
    def productExceptSelf(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        res = [1] * len(nums)
        for i in range(1, len(nums), 1):
            res[i] = res[i-1] * nums[i-1]
        rProd = 1
        for i in range(len(nums)-1, -1, -1):
            res[i] *= rProd
            rProd *= nums[i]
        return res