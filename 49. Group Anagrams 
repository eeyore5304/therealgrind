class Solution(object):
    def groupAnagrams(self, strs):
        """
        :type strs: List[str]
        :rtype: List[List[str]]
        """
        res = []
        str_dict = {}
        for word in strs:
            sorted_word = "".join(sorted(word))
            if sorted_word in str_dict:
                str_dict[sorted_word].append(word)
            else:
                str_dict[sorted_word] = [word]
        for key,val in str_dict.items():
            res.append(val)
        return res